#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

BUILD_DIR=$1
CACHE_DIR=$2
BIN_DIR=$(cd "$(dirname "$0")"; pwd) # absolute path


echo ""
echo $BUILD_DIR
find $BUILD_DIR -type f -name 'test.png' 2>/dev/null

find / -type f -name 'test.png' 2>/dev/null
pwd

FROMPATH=$BUILD_DIR/src/white-label/skov-sponsor/image-replacement/

rsync -r $FROMPATH $BUILD_DIR/build/static/media

ls $BUILD_DIR/build/static/media
